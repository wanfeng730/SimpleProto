<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.wanfeng.sp.base.mapper.SpBaseObjectMapper">

    <sql id="all_field">
        id, type, name, create_date, modify_date, is_delete, data
    </sql>

    <select id="findById" resultMap="mybatis-plus_SpBaseObjectDO">
        select
        <include refid="all_field"/>
        from sp_object_table
        where id = #{id}
    </select>

</mapper>