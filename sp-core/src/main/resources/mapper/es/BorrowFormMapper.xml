<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.wanfeng.sp.base.mapper.es.BorrowFormMapper">

    <!-- 设置表字段 -->
    <!--
    修改字段后需要同步修改本文件中的sql: insertSettings、updateSettings -->
    <sql id="all_field">
        id, name, form_no, create_date
    </sql>

    <select id="findById" resultMap="mybatis-plus_BorrowFormDO">
        select
        <include refid="all_field"/>
        from sp_object_table
        where id = #{id}
    </select>




</mapper>